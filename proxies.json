{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "proxy1": {
      "matchCondition": {
          "methods": [ "GET" ,"POST"],
          "route": "/{Action}"
      },
      "backendUri": "https://ssvm4a.azurewebsites.net/api/HttpTrigger",
      "requestOverrides": {
        "backend.request.method":"POST",
        "backend.request.querystring.Action": "{Action}"
    }
    }
  }
}
